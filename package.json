{
  "name": "MooD-Custom-CodeBase-Babel",
  "version": "1.0.0",
  "description": "Babel version of custom codebase",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "build": "rimraf ./Code/js/* && babel Code/src --out-dir Code/js --extensions \".ts,.tsx\"",
    "compile": "rimraf ./Code/dist && webpack --config ./webpack.config.js --json > stats.json && babel Code/dist --out-file Code/dist/babelBundle.js --extensions \".js\"",
    "document": "rimraf MooD-Custom-CodeBase-Babel-Ts.wiki/* && typedoc --tsconfig ./Code/src/tsconfig.json && copyfiles -u 1 Wiki_Resources/* MooD-Custom-CodeBase-Babel-Ts.wiki/",
    "clearWiki": "rimraf MooD-Custom-CodeBase-Babel-Ts.wiki/* ",
    "check-types": "tsc --noEmit",
    "test-browser": "webpack build --config ./spec/support/jasmineWebpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-stage-3": "^7.8.3",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jasmine": "^4.0.2",
    "@types/jquery": "^3.5.14",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "copyfiles": "^2.4.1",
    "devextreme": "^21.2.6",
    "jquery": "^3.6.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.12",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
